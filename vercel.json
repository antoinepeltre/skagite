{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/skagite",
    "routes": [
        { "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg)", "headers": { "cache-control": "max-age=3600" } },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "framework": "angular"
}